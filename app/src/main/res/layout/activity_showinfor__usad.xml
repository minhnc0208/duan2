<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:background="#579C95"

tools:context=".activityadmin.Showinfor_UsadActivity">
<ImageView
    android:id="@+id/nentren"
    android:layout_width="match_parent"
    android:layout_height="274dp"
    android:layout_marginTop="-50dp"
    android:src="@drawable/food"></ImageView>

    <ImageView
        android:id="@+id/nentren1"
        android:layout_width="match_parent"
        android:layout_height="264dp"
        android:layout_marginTop="-12dp"
        android:src="@drawable/nenbackgroundtren"></ImageView>

<ImageView
    android:id="@+id/nenduoi"
    android:layout_width="match_parent"
    android:layout_height="531dp"
    android:layout_marginTop="230dp"></ImageView>

<ImageView
    android:id="@+id/avatar"
    android:layout_width="80dp"
    android:layout_height="80dp"
    android:layout_centerHorizontal="true"
    android:layout_marginTop="110dp"
    android:background="@color/white"
    android:foreground="@drawable/lg"></ImageView>
<ImageView
    android:id="@+id/vien"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginLeft="-46dp"
    android:layout_marginTop="-75dp"
    android:contentDescription="TODO"
    android:src="@drawable/logo"></ImageView>
<ImageView
    android:id="@+id/back"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:src="@drawable/back"
    android:layout_marginTop="15dp"></ImageView>

    <ImageView
        android:id="@+id/font1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/nentren1"
        android:layout_centerHorizontal="true"
        android:background="@drawable/nencustomer" />

    <ImageButton
        android:id="@+id/btned"
        style="@android:style/Widget.Material.Light.ImageButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/font1"
        android:layout_alignRight="@+id/font1"
        android:layout_marginTop="-80dp"
        android:layout_marginRight="80dp"
        android:background="@drawable/btnedit" />

    <ImageButton
        android:id="@+id/btncen"
        style="@style/Widget.AppCompat.ImageButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/font1"
        android:layout_alignLeft="@+id/font1"
        android:layout_marginLeft="80dp"
        android:layout_marginTop="-80dp"
        android:background="@drawable/btndelete" />

    <TextView
        android:id="@+id/tvusname"
        android:layout_width="260dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/nentren1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/roboto"
        android:gravity="center"
        android:hint="abc"
        android:textSize="25dp"
        android:textColor="#FFBF00"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/tvfullname"
        android:layout_width="260dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvusname"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/roboto"
        android:hint="Họ và tên:"
        android:textSize="15dp" />

    <TextView
        android:id="@+id/tvfullname1"
        android:layout_width="260dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvfullname"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/roboto"
        android:hint="abc"
        android:textAlignment="center"
        android:textStyle="bold"
        android:textColor="#FFBF00"
        android:textSize="20dp" />


    <TextView
        android:id="@+id/tvphone"
        android:layout_width="260dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvfullname1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/roboto"
        android:hint="Số điện thoại"
        android:textSize="15dp" />
    <TextView
        android:id="@+id/tvphone1"
        android:layout_width="260dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvphone"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/roboto"
        android:hint="abc"
        android:textColor="#FFBF00"
        android:textAlignment="center"
        android:textStyle="bold"
        android:textSize="20dp" />
    <TextView
        android:id="@+id/tvgender"
        android:layout_width="260dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvphone1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/roboto"
        android:hint="Giới tính:"
        android:textSize="15dp" />

    <TextView
        android:id="@+id/tvgender1"
        android:layout_width="260dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvgender"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/roboto"
        android:hint="abc"
        android:textColor="#FFBF00"
        android:textAlignment="center"
        android:textStyle="bold"
        android:textSize="20dp" />
    <TextView
        android:id="@+id/tvrule"
        android:layout_width="260dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvgender1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/roboto"
        android:hint="Quyền sử dụng:"
        android:textSize="15dp" />

    <TextView
        android:id="@+id/tvrule1"
        android:layout_width="260dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvrule"
        android:textColor="#FFBF00"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/roboto"
        android:hint="abc"
        android:textAlignment="center"
        android:textStyle="bold"
        android:textSize="20dp" />
</RelativeLayout>